{
  "name": "@auditswarm.xyz/client",
  "version": "0.1.0",
  "description": "TypeScript client for AuditSwarm API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auditswarm/auditswarm"
  },
  "keywords": ["solana", "crypto", "tax", "compliance", "audit"],
  "license": "MIT"
}
